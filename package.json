{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "chmod +x ./node_modules/.bin/hexo && npx hexo generate",
    "build:win": "npx hexo generate",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "server": "hexo server",
    "test": "vitest run",
    "perf:baseline": "lhci collect --config=tests/perf/lhci.json --url=http://localhost:4000/",
    "perf:assert": "lhci assert --config=tests/perf/lhci.json",
    "perf:upload": "lhci upload --config=tests/perf/lhci.json"
  },
  "hexo": {
    "version": "7.3.0"
  },
  "dependencies": {
    "hexo-abbrlink": "^2.2.1",
    "hexo-bilibili-bangumi": "^2.0.1",
    "hexo-blog-encrypt": "^3.1.9",
    "hexo-butterfly-envelope": "^1.0.15",
    "hexo-clean-css": "^2.0.0",
    "hexo-deployer-git": "^4.0.0",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^3.0.0",
    "hexo-generator-tag": "^2.0.0",
    "hexo-html-minifier-terser": "^1.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^6.0.0",
    "hexo-renderer-pug": "^3.0.0",
    "hexo-renderer-stylus": "^3.0.1",
    "hexo-server": "^3.0.0",
    "hexo-theme-landscape": "^1.0.0"
  },
  "devDependencies": {
    "@lhci/cli": "^0.12.0",
    "hexo": "^7.3.0",
    "jsdom": "^24.1.0",
    "vitest": "^4.0.15"
  }
}
